{
  "name": "conversational-ai-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Conversational AI Outbound Call System",
  "scripts": {
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev",
    "backend:dev": "cd backend && node --watch src/server.js",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "echo Please run 'npm run backend' and 'npm run frontend' in separate terminals"
  },
  "keywords": [
    "conversational-ai",
    "elevenlabs",
    "outbound-calls"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.1",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}
